<div class="card mb-4">
  <div class="row">
    <div class="col-md-3">
      <img
        src="/static/images/<%= image %>"
        class="img-fluid h-100"
        alt="<%= image %>" />
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title">
          <a style="text-decoration: none" href="/blogs/<%=url%>">
            <%= title %>
          </a>
        </h5>
        <p class="card-text"><%= subTitle %></p>
      </div>
    </div>
  </div>
</div>
